{
  "extends": [
    "airbnb",
    "prettier"
  ],
  "rules": {
   "no-console": "off",
   "import/no-extraneous-dependencies": ["error", {"devDependencies": true, "optionalDependencies": false, "peerDependencies": false}]
},
  "globals": {
    "afterAll",
    "beforeAll",
    "beforeEach",
    "afterEach",
    "describe",
    "fit",
    "it",
    "jasmine",
    "chai",
    "expect",
    "xit",
    "jest"
  }
}
